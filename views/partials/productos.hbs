{{#each producto}}
  <div class="container-fluid col-md-4">
    <div class="card" style="width: 18rem;">
      <img class="card-img-top" src="/img/{{this}}.png" alt="Card image cap" />
      <div class="card-body">
        <h5 class="card-title">{{this}}</h5>
      </div>
    </div>
  </div>
{{/each}}